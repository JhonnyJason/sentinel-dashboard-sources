#chart-instructions
    display none
    color white
    padding 20px
    box-sizing: border-box 
    max-width 888px
    margin auto
    h2
        font-size 22pt
        margin 20px 0
        text-align: left
    ul
        list-style-type: disc
        text-align left
        margin-left 30px
    li
        margin-bottom 5px
        font-size: 13pt
    li.warning
        color cd-danger-red
    p
        text-align: left
        font-size 14pt
        margin 15px 0
    