// placeholderframe styles
.placeholderframe
    width calc(100% - 50px)
    min-height 400px
    display flex
    justify-content center
    align-items center
    margin 25px
    background-color alpha(cd-dark-blue, 0.1)
    border-radius 3px
    .coming-soon
        font-size 18px
        color #666
        text-align center
        line-height: 130%